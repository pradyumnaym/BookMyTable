<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bookmytable">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".Firebase.FireBase"
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ThankYouActivity"></activity>
        <activity android:name=".enterdetails" />
        <activity android:name=".booknow" />
        <activity android:name=".MyBookings" />
        <activity android:name=".CuisineListActivity" />
        <activity android:name=".RestaurantListActivity" />
        <activity android:name=".restaurantCards" />
        <activity android:name=".signup" />
        <activity android:name=".credits" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>